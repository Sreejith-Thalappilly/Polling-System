{
  "name": "polling-system",
  "version": "1.0.0",
  "description": "A comprehensive polling system with Express.js backend and role-based access control",
  "author": "Sreejith Thalappilly",
  "private": true,
  "license": "ISC",
  "scripts": {
    "start": "node src/app.js",
    "start:dev": "nodemon src/app.js",
    "start:debug": "node --inspect src/app.js",
    "frontend": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:preview": "cd frontend && npm run preview",
    "dev": "concurrently \"npm run start:dev\" \"npm run frontend\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.1.5",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.2",
    "multer": "^1.4.5-lts.1",
    "express-fileupload": "^1.4.3",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "eslint": "^8.42.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sreejith-Thalappilly/Polling-System.git"
  },
  "bugs": {
    "url": "https://github.com/Sreejith-Thalappilly/Polling-System/issues"
  },
  "homepage": "https://github.com/Sreejith-Thalappilly/Polling-System#readme"
}